{
  <%_ if (cloudFunc) {_%>
  "cloudfunctionRoot": "functions",
  "compileType": "miniprogram",
  <%_ } _%>
  <%_ if (isPlugin) {_%>
  "pluginRoot": "plugin",
  "compileType": "plugin",
  <%_ } _%>
  "setting": {
    "newFeature": true,
    "urlCheck": true,
    "es6": false,
    "postcss": true,
    "minified": false,
    "checkSiteMap": false,
    "autoAudits": false
  },
  "appid": "<%= appid %>",
  "projectname": "<%= name %>",
  "description": "<%= description %>",
  "condition": {}
}
